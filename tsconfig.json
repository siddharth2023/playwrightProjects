{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
            "@fixtures/*": ["libs/*"]
        },
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "node",
        "sourceMap": false,
        "skipLibCheck": true,
        "typeRoots": ["./node_modules/@types"]
    },
    "exclude": ["./extensions", "./node_modules"]
}
